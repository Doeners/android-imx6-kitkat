+	private static final Comparator type = new Comparator<String>() {
+		@Override
+		public int compare(String arg0, String arg1) {
+			String ext = null;
+			String ext2 = null;
+			
+			try {
+				ext = arg0.substring(arg0.lastIndexOf(".") + 1, arg0.length());
